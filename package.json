{
  "name": "@erock530/react-iframe",
  "version": "1.0.0",
  "description": "A lightweight React component for rendering iframes with custom headers",
  "author": {
    "name": "Erock530",
    "email": "56282917+erock530@users.noreply.github.com",
    "url": "https://github.com/erock530"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erock530/react-iframe.git"
  },
  "bugs": {
    "url": "https://github.com/erock530/react-iframe/issues"
  },
  "homepage": "https://github.com/erock530/react-iframe#readme",
  "keywords": [
    "react",
    "iframe",
    "react-component",
    "headers",
    "authentication",
    "embed",
    "custom-headers"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
