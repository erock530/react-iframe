{
  "name": "@erock530/react-iframe",
  "version": "1.0.0",
  "description": "A simple react component that lets you add headers to an iframe.",
  "author": {
    "name": "Erock530",
    "email": "56282917+erock530@users.noreply.github.com",
    "url": "https://github.com/erock530"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erock530/react-iframe.git"
  },
  "keywords": [
    "iframe",
    "react-iframe"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
